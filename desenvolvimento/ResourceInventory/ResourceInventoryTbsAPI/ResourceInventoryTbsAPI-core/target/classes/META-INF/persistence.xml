<?xml version="1.0" encoding="ISO-8859-1"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">	
 	
 	<persistence-unit name="TbsDS" transaction-type="JTA">
        <jta-data-source>jdbc/TbsDS</jta-data-source>
		<class>com.tlf.oss.resourceinventory.tbs.entity.AllocateEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.DeallocateEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.GatherResourceEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.InstallEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.NetworkInfoEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.ReserveEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.RetrieveAssignedTelNumEntity</class>
		<class>com.tlf.oss.resourceinventory.tbs.entity.DetermineAvailabilityEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.RetrieveEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.RetrieveNetworkLocationEntity</class>
        <class>com.tlf.oss.resourceinventory.tbs.entity.v2_0.RetrieveFacilityEntity</class>

		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="eclipselink.query-results-cache" value="false"/>
		</properties>

    </persistence-unit>
</persistence>