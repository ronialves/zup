<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://java.sun.com/xml/ns/persistence
        http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="GraniteServicePU"
		transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/arquillian</jta-data-source>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveUdaEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveAvailabilityMsanEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveAvailabilityDslamEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.ReserveResourceEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.ReserveResourceGponEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveAccessResourceInformationMsanEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveAccessResourceInformationDslamEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.CancelResourceEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveFacilityEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.AvailabilityRetrieveActiveMsanDslamEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.DeallocateInternalResourceEntity</class>
		<class>com.tlf.oss.resourceinventory.granite.entity.RetrieveFacilityGponEntity</class>
		<properties>
			<property name="eclipselink.allow-zero-id" value="true" />
		</properties>
	</persistence-unit>
</persistence>
