<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-ejb-jar
	xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-ejb-jar"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
						http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd 
						http://xmlns.oracle.com/weblogic/weblogic-ejb-jar 
						http://xmlns.oracle.com/weblogic/weblogic-ejb-jar/1.4/weblogic-ejb-jar.xsd">
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>AllocateAndInstallResourceWS</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:transaction-descriptor>
            <wls:trans-timeout-seconds>300</wls:trans-timeout-seconds>
        </wls:transaction-descriptor>
        <wls:dispatch-policy>ResourceInventoryWorkManager</wls:dispatch-policy>
        <wls:remote-client-timeout>300</wls:remote-client-timeout>
    </wls:weblogic-enterprise-bean>
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>ResourceDataCollectionV2WS</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:transaction-descriptor>
            <wls:trans-timeout-seconds>300</wls:trans-timeout-seconds>
        </wls:transaction-descriptor>
        <wls:dispatch-policy>ResourceInventoryWorkManager</wls:dispatch-policy>
        <wls:remote-client-timeout>300</wls:remote-client-timeout>
    </wls:weblogic-enterprise-bean>
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>ResourceDataCollectionWS</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:transaction-descriptor>
            <wls:trans-timeout-seconds>300</wls:trans-timeout-seconds>
        </wls:transaction-descriptor>
        <wls:dispatch-policy>ResourceInventoryWorkManager</wls:dispatch-policy>
        <wls:remote-client-timeout>300</wls:remote-client-timeout>
    </wls:weblogic-enterprise-bean>	
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>FaultManagementResourceDataCollectionWS</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:transaction-descriptor>
            <wls:trans-timeout-seconds>300</wls:trans-timeout-seconds>
        </wls:transaction-descriptor>
        <wls:dispatch-policy>FaultManagement</wls:dispatch-policy>
        <wls:remote-client-timeout>300</wls:remote-client-timeout>
    </wls:weblogic-enterprise-bean>
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>GatherAndAnalyseResourceInformationWS</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:transaction-descriptor>
            <wls:trans-timeout-seconds>300</wls:trans-timeout-seconds>
        </wls:transaction-descriptor>
        <wls:dispatch-policy>ResourceLifeCycleManagement</wls:dispatch-policy>
        <wls:remote-client-timeout>300</wls:remote-client-timeout>
    </wls:weblogic-enterprise-bean>
</wls:weblogic-ejb-jar>